    "personInformation": {
        "dialogContentHead": {
            "text": "個人情報"
        },
        "personName": {
            "textboxLabel": {
                "personLabel": {
                    "html": "氏名<br>"
                },
                "personTextbox": {
                    "name": "name",
                    "size": "27",
                    "autocomplete": "off",
                    "type": "text",
                    "align": "right"
                }
            }
        },
        "personNameKana": {
            "textboxLabel": {
                "personLabel": {
                    "html": "氏名(カナ)<br>"
                },
                "personTextbox": {
                    "name": "nameKana",
                    "size": "27",
                    "autocomplete": "off",
                    "type": "text",
                    "align": "right"
                }
            }
        },
        "personPhoneNumber": {
            "textboxLabel": {
                "personLabel": {
                    "html": "電話番号<br>"
                },
                "personTextbox": {
                    "name": "personPhoneNumber",
                    "size": "27",
                    "style": "ime-mode:disabled",
                    "autocomplete": "off",
                    "type": "tel",
                    "onkeyDown": "return controllInputChar(event);"
                }
            }
        },
        "personEmail": {
            "textboxLabel": {
                "personLabel": {
                    "html": "メールアドレス<br>"
                },
                "personTextbox": {
                    "name": "email",
                    "size": "27",
                    "autocomplete": "off",
                    "style": "ime-mode:disabled",
                    "type": "email"
                }
            }
        },
        "personEmailCheck": {
            "textboxLabel": {
                "personLabel": {
                    "html": "メールアドレス(確認)<br>"
                },
                "personTextbox": {
                    "class": "personTextbox",
                    "name": "personEmailCheck",
                    "size": "27",
                    "style": "ime-mode:disabled",
                    "autocomplete": "off",
                    "type": "email",
                    "oncopy": "return false",
                    "onpaste": "return false",
                    "ononcontextmenu": "return false",
                    "placeholder":"手動で入力してください。"
                }
            }
        },
        "personCount": {
            "textboxLabel": {
                "personLabel": {
                    "html": "人数<br>"
                },
                "personTextbox": {
                    "name": "personCount",
                    "size": "2",
                    "style": "ime-mode:disabled",
                    "autocomplete": "off",
                    "onkeyDown": "return controllInputChar(event);",
